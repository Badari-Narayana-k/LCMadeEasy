{
  "manifest_version": 3,
  "name": "LeetCode AI Interviewer",
  "version": "2.0",
  "description": "Acts like an intelligent interviewer while solving LeetCode problems.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": ["http://localhost:11434/*"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
